<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mgnt.test.center.entity">
	<class name="TbCategory" table="tb_category">
					<id name="SID" column="SID" type="java.lang.Integer">
						<generator class="identity" />
					</id>
				    <property name="CATEGORY_NAME" column="CATEGORY_NAME" not-null="false" />
				    <property name="IS_SHOW" column="IS_SHOW" not-null="false" />
				    <property name="C_TIME" column="C_TIME" not-null="false" />
				    <property name="PARENT_SID" column="PARENT_SID" not-null="false" />
				    <property name="LEVEL" column="LEVEL" not-null="false" />
				    <property name="SHOW_POSITION" column="SHOW_POSITION" not-null="false" />
				    <property name="SHOW_VIEW" column="SHOW_VIEW" not-null="false" />
	</class>
</hibernate-mapping>