<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mgnt.test.center.entity">
	<class name="TbCourse" table="tb_course">
					<id name="SID" column="SID" type="java.lang.Integer">
						<generator class="identity" />
					</id>
				    <property name="COS_NAME" column="COS_NAME" not-null="false" />
				    <property name="CTIME" column="CTIME" not-null="false" />
				    <property name="UTIME" column="UTIME" not-null="false" />
	</class>
</hibernate-mapping>