package com.rapidweb2.common.exception;

/**
 * @author chis(chis123@qq.com)
 *
 */
public class WebException extends RuntimeException {
	private static final long serialVersionUID = -5950761931807183662L;
	public WebException(){
		super();
	}
	public WebException(String msg,Object...params){
		super( String.format(msg, params) );
	}
	public WebException(Throwable e){
		super(e);
	}
	public WebException(Throwable e,String msg,Object...params){
		super(String.format(msg, params),e);
	}

}
