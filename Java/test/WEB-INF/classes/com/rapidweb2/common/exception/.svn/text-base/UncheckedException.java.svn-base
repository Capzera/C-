package com.rapidweb2.common.exception;

/**
 * @author chis(chis123@qq.com)
 *
 */
@SuppressWarnings("serial")
public class UncheckedException extends RuntimeException {
	public UncheckedException(){
		super();
	}
	public UncheckedException(String msg,Object...params){
		super( String.format(msg, params) );
	}
	public UncheckedException(Throwable e){
		super(e);
	}
	public UncheckedException(Throwable e,String msg,Object...params){
		super(String.format(msg, params),e);
	}
}
