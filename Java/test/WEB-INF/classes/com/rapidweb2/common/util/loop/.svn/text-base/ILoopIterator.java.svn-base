/**
 * 作为遍历
 * @author chis(chis123@qq.com)
 *
 */
public interface ILoopIterator<T> {
	/**
	 * 执行每一项
	 * @param item
	 * @param index
	 * @return 是否继续循环，true = 继续循环，false = 不再循环
	 */
	public boolean process(T item ,int index );
}
