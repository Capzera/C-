package com.rapidweb2.common.util;

/**
 * Object辅助工具类
 * 
 * @author CHI
 */
public class ObjectUtil
{
	/* 转换为字符类型 */
	@SuppressWarnings("unchecked")
	public static <T> T convert(Object o, T defval)
	{
		if (o == null) return defval;
		try
		{
			return (T) o;
		}
		catch (Throwable e)
		{
			return defval;
		}
	}
	public static java.sql.Date toSqlDate(java.util.Date d){
		if( d==null ) return null;
		return new java.sql.Date( d.getTime() );
	}
	
	public static java.util.Date toUtilDate(java.sql.Date d){
		if( d==null ) return null;
		return new java.sql.Date( d.getTime() );
	}
}
