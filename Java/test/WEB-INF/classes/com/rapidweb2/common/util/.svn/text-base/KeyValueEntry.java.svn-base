package com.rapidweb2.common.util;

/**
 * 由KEY-VALUE构成的一组值
 * @author chis(chis123@qq.com)
 *
 */
public class KeyValueEntry<K, V> {
	private K key;
	private V value;
	
	public KeyValueEntry(){}
	
	public KeyValueEntry(K key,V value){
		this.key  = key;
		this.value = value;
	}
	
	public K getKey() {
		return this.key;
	}
	public void setKey(K key) {
		this.key = key;
	}
	public V getValue() {
		return this.value;
	}
	public void setValue(V value) {
		this.value = value;
	}
	
}
