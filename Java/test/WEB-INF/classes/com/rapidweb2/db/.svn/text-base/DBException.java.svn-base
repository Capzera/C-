package com.rapidweb2.db;

import org.springframework.dao.DataAccessException;

/**
 * 数据库操作异常类
 */
@SuppressWarnings("serial")
public class DBException extends DataAccessException {

	public DBException(String msg,Object...params){
		super( String.format(msg, params) );
	}

	public DBException(Throwable e,String msg,Object...params){
		super(String.format(msg, params),e);
	}

}
